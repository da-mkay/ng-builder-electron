{
    "name": "@da-mkay/ng-builder-electron",
    "version": "0.0.1",
    "description": "A set of builders for the Angular CLI to build electron apps.",
    "repository": {
        "type": "git",
        "url": "https://github.com/da-mkay/ng-builder-electron.git"
    },
    "homepage": "https://github.com/da-mkay/ng-builder-electron",
    "scripts": {
        "build": "npm run clean && tsc -p tsconfig.json && npm run copy:json",
        "clean": "rimraf dist",
        "copy:json": "cpx 'src/**/*.json' ./dist"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern '**/*.ts'"
        }
    },
    "keywords": [
        "angular",
        "ng",
        "electron",
        "builder",
        "electron-builder",
        "watch"
    ],
    "author": "da-mkay (https://github.com/da-mkay)",
    "license": "MIT",
    "peerDependencies": {
        "@angular-devkit/architect": "^0.1102.0",
        "@angular-devkit/core": "^11.2.0",
        "rxjs": "6.6.3"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.1102.0",
        "@angular-devkit/core": "^11.2.0",
        "@types/fs-extra": "^9.0.7",
        "@types/node": "^14.14.26",
        "cpx": "^1.5.0",
        "husky": "^5.0.9",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "rimraf": "^3.0.2",
        "rxjs": "6.6.3",
        "typescript": "^4.1.5"
    },
    "dependencies": {
        "ansi-colors": "^4.1.1",
        "depcheck": "^1.4.0",
        "electron-builder": "^22.9.1",
        "fs-extra": "^9.1.0"
    },
    "builders": "./dist/builders/builders.json"
}
